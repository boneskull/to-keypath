{
  "name": "to-keypath",
  "version": "0.1.0",
  "type": "module",
  "description": "Convert an array of object keys to a keypath string",
  "author": {
    "name": "Christopher Hiller",
    "email": "boneskull@boneskull.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boneskull/to-keypath.git"
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "types": "./dist/commonjs/index.d.ts",
  "keywords": [
    "string",
    "object",
    "keys",
    "props",
    "keypath",
    "dot",
    "bracket",
    "notation",
    "nested",
    "property",
    "cli",
    "terminal",
    "console"
  ],
  "scripts": {
    "test": "node --import tsx --test ./test/index.spec.ts",
    "build": "tshy",
    "format": "prettier . --write",
    "lint": "eslint . --max-warnings 0",
    "lint:commit": "commitlint",
    "lint:format": "prettier . --check",
    "lint:knip": "knip",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \".github/**/*.md\"",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "lint:staged": "lint-staged",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "is-identifier": "1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.10.1",
    "cspell": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "husky": "^9.1.7",
    "knip": "^5.37.1",
    "lint-staged": "^15.2.10",
    "markdownlint-cli2": "^0.15.0",
    "markdownlint-cli2-formatter-pretty": "^0.0.7",
    "prettier": "^3.3.3",
    "prettier-plugin-curly": "^0.3.1",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-pkg": "^0.18.1",
    "prettier-plugin-sh": "^0.14.0",
    "tshy": "3.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  }
}
