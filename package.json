{
  "name": "to-keypath",
  "version": "0.1.2",
  "type": "module",
  "description": "Convert an array of object keys to a keypath string",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boneskull/to-keypath.git"
  },
  "author": {
    "name": "Christopher Hiller",
    "email": "boneskull@boneskull.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.19.0"
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "src",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "string",
    "object",
    "keys",
    "props",
    "keypath",
    "dot",
    "bracket",
    "notation",
    "nested",
    "property",
    "cli",
    "terminal",
    "console"
  ],
  "scripts": {
    "build": "tshy",
    "format": "prettier . --write",
    "lint": "eslint . --max-warnings 0",
    "lint:commit": "commitlint",
    "lint:format": "prettier . --check",
    "lint:knip": "knip",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \".github/**/*.md\"",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "lint:staged": "lint-staged",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "node --import tsx --test ./test/index.spec.ts",
    "tsc": "tsc -p tsconfig.tsc.json"
  },
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin": "5.6.1",
    "bupkis": "0.14.0",
    "cspell": "9.4.0",
    "eslint": "9.39.2",
    "eslint-plugin-perfectionist": "5.0.0",
    "husky": "9.1.7",
    "knip": "5.75.1",
    "lint-staged": "16.2.7",
    "markdownlint-cli2": "0.20.0",
    "markdownlint-cli2-formatter-pretty": "0.0.9",
    "prettier": "3.7.4",
    "prettier-plugin-curly": "0.4.1",
    "prettier-plugin-jsdoc": "1.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-pkg": "0.21.2",
    "prettier-plugin-sh": "0.18.0",
    "tshy": "3.1.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ],
      "footer-max-line-length": [
        0
      ],
      "header-max-length": [
        0
      ],
      "subject-case": [
        0
      ],
      "subject-full-stop": [
        0
      ]
    }
  },
  "knip": {
    "entry": [
      "src/index.ts",
      "test/**/*.spec.ts"
    ],
    "ignoreDependencies": [
      "markdownlint-cli2-formatter-pretty"
    ],
    "ignoreExportsUsedInFile": {
      "interface": true,
      "type": true
    },
    "project": [
      "src/**/*.ts",
      "test/**/*.ts"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write",
      "cspell lint --no-must-find-files"
    ],
    "*.md": "markdownlint-cli2",
    "*.ts": "eslint --fix"
  },
  "markdownlint-cli2": {
    "config": {
      "ul-style": {
        "style": "dash"
      },
      "ul-indent": {
        "indent": 2
      },
      "commands-show-output": false,
      "no-bare-urls": false,
      "line-length": false,
      "no-emphasis-as-header": false,
      "no-duplicate-header": false,
      "header-increment": false,
      "no-blanks-blockquote": false,
      "headings": false
    },
    "ignores": [
      "**/node_modules/**",
      "**/dist/**",
      ".github/CODE_OF_CONDUCT.md",
      "**/CHANGELOG.md"
    ],
    "outputFormatters": [
      [
        "markdownlint-cli2-formatter-pretty",
        {
          "appendLink": true
        }
      ]
    ],
    "showFound": true,
    "noProgress": false
  },
  "prettier": {
    "overrides": [
      {
        "files": ".nvmrc",
        "options": {
          "parser": "yaml"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-curly",
      "prettier-plugin-sh",
      "prettier-plugin-organize-imports",
      "prettier-plugin-pkg",
      "prettier-plugin-jsdoc"
    ],
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "endOfLine": "auto",
    "jsdocCommentLineStrategy": "multiline",
    "tsdoc": true
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  }
}
